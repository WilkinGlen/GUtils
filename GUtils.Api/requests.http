  "id": 456,
  "name": "Complex Test",
  "nestedData": {
    "tags": ["tag1", "tag2", "tag3"],
    "categories": ["cat1", "cat2"],
    "permissions": ["read", "write", "execute"]
  },
  "items": ["item-a", "item-b", "item-c"],
  "createdAt": "2024-01-15T10:30:00Z"
}

